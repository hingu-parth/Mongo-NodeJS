<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>List of Orders</title>
  </head>
  <body>
      <h2>Admin Dashboard</h2>
    <table border="1" cellpadding="10" cellspacing="0">
      <thead>
        <tr>
          <th>Id</th>
          <th>Customer Name</th>
          <th>Address</th>
          <th>Email</th>
          <th>Item Name</th>
          <th>Item Price</th>
          <th>Status</th>
        </tr>
      </thead>
      <tbody>
          <% for(let i=0; i<orders.length;i++){ %>

          <tr>
            <td><%= orders[i]._id %></td>
            <td><%= orders[i].name %></td>
          <td><%= orders[i].address %></td>
          <td><%= orders[i].email %></td>
          <td><%= orders[i].productName %></td>
          <td><%= orders[i].productPrice %></td>
          <td><%= orders[i].status %></td></tr>
          <% } %>
      </tbody>
    </table>
  </body>
  <br>
  <a  href="/" name='back'  >Back</a>
</html>
